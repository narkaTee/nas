[Exec]
PrivateUsers=pick
ResolvConf=bind-host
{% if container.capabilities is defined %}
Capability={{ container.capabilities | join(' ') }}
{% endif %}

[Network]
VirtualEthernet=yes
IPMasquerade=yes
{% for port in container.ports | default([]) %}
Port={{ port }}
{% endfor %}
{% if container.bridge is defined %}
Bridge={{ container.bridge }}
{% endif %}
